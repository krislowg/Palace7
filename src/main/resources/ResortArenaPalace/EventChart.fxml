<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ResortArenaPalace.EventChart">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1000.0" opacity="0.37" pickOnBounds="true">
         <image>
            <Image url="@Login_PalmTree.jpg" />
         </image>
      </ImageView>
      <BarChart fx:id="eventBarChart" layoutX="110.0" layoutY="100.0" prefHeight="453.0" prefWidth="795.0" stylesheets="@ArenaStyle.css" title="Events overview">
        <xAxis>
          <CategoryAxis label="Month" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Reservations" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="btn_GoBackEvChart" layoutX="31.0" layoutY="30.0" mnemonicParsing="false" onAction="#changeEventChartToEvReport" stylesheets="@ArenaStyle.css" text="Go Back" />
   </children>
</AnchorPane>
