<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ResortArenaPalace.BarChartEx">
   <ImageView fitHeight="600.0" fitWidth="1000.0" opacity="0.37" pickOnBounds="true">
      <image>
         <Image url="@Login_PalmTree.jpg" />
      </image>
   </ImageView>
    <BarChart fx:id="barChart_Booking" layoutX="65.0" layoutY="89.0" prefHeight="425.0" prefWidth="862.0" title="Booking Overview">
        <xAxis>
            <CategoryAxis label="Months" side="BOTTOM" fx:id="x_Months" />
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="y_Reservation" label="Reservations" side="LEFT" />
        </yAxis>
    </BarChart>
   <Button fx:id="btn_GuestChart" layoutX="42.0" layoutY="37.0" mnemonicParsing="false" onAction="#changeGuestChartToReport" stylesheets="@ArenaStyle.css" text="Go Back" />
</AnchorPane>
